{
    "name": "Update/laravel-paystack",
    "description": "A Laravel Package for Paystack",
    "keywords": ["php","github", "laravel","Open Source","payments", "subscription", "paystack", "paystack.co","laravel 5", "laravel 6"],
    "license": "MIT",
    "authors": [
      {
        "name": "Test",
        "email": "test@gmail.com"
      }
   ],
   "require": {
    "php": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0",
    "illuminate/support": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0",
    "guzzlehttp/guzzle": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0"
   },
   "require-dev": {
        "phpunit/phpunit" : "~5.0||~6.0||~7.0||~8.0",
        "scrutinizer/ocular": "~1.1",
        "satooshi/php-coveralls": "^0.7.0",
        "mockery/mockery": "^1.0"
    },
   "autoload": {
        "psr-4": {
          "Update\\Paystack\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "Update\\Paystack\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
   "extra": {
    "laravel": {
        "providers": [
            "Update\\Paystack\\PaystackServiceProvider"
        ],
        "aliases": {
            "Paystack": "Update\\Paystack\\Facades\\Paystack"
        }
    }
  }
}
